{
	"expo": {
		"name": "Mobile Flashcards",
		"description": "This app allows users to create flashcards to quiz themselves.",
		"slug": "mobile-flashcards",
		"privacy": "public",
		"sdkVersion": "36.0.0",
		"platforms": [
			"ios",
			"android",
			"web"
		],
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/icon-48.png",
		"splash": {
			"image": "./assets/splash.png",
			"resizeMode": "contain",
			"backgroundColor": "#ffffff"
		},
		"updates": {
			"fallbackToCacheTimeout": 0
		},
		"assetBundlePatterns": [
			"**/*"
		],
		"ios": {
			"supportsTablet": true
		}
	}
}
